{{>layout/header}}
<section id="main" class="wrapper">
    <div class="container">
        <header class="major">
            <h2>정보 수정</h2>
        </header>
        <div class="col-md-12">
            <div class="col-md-4">
                <form>
                    <input type="hidden" id="id" value="{{user_id}}">
                    <p><div class="form-group">
                    <label for="이름">이름</label>
                    <input type="text" class="form-control" id="name" placeholder="아이디">
                </div></p>
                    <p><div class="form-group">
                    <label for="비밀번호">비밀번호</label>
                    <input type="passwrd" class="form-control" id="pw" placeholder="패스워드">
                </div></p>
                    <p><div class="form-group">
                    <label for="비밀번호 확인">비밀번호 확인</label>
                    <input type="passwrd" class="form-control" id="pwcheck" placeholder="패스워드">
                </div></p>
                    <button type="button" id="update">수정</button>
                </form>

            </div>
        </div>
    </div>
</section>

<!-- Footer -->
<footer id="footer">
</footer>

{{>layout/footer}}
